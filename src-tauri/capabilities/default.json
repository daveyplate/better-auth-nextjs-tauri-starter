{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*:*"
        },
        {
          "url": "http://*:*"
        }
      ]
    },
    "core:default",
    "os:default",
    "deep-link:default",
    "opener:default"
  ]
}
{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "export": {
            "dependsOn": [
                "^export"
            ],
            "outputs": [
                "out/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "check-types": {
            "dependsOn": [
                "^check-types"
            ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    },
    "envMode": "loose"
}
